{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "build",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHsc",
        "/nologo",
        "/arch:AVX2",
        "/openmp",
        "/Fe${workspaceFolder}\\out\\${fileBasenameNoExtension}.exe",
        "/Fo${workspaceFolder}\\out\\",
        "/Fd${workspaceFolder}\\out\\",
        "${file}"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger.",
      "presentation": {
        "reveal": "silent",
        "revealProblems": "onProblem",
      }
    }
  ],
  "version": "2.0.0"
}